<% content_for :head_left do %>
  <h2><%= t('desserts.new.title')%></h2>
<% end %>
<% content_for :head_right do %>
  <h2><%= link_to t('forms.buttons.back'), @dessert.desserts_category, class: "btn btn-default pull-right" %></h2>
<% end %>

<%= form_for(@dessert, url: { action: "create" }) do |f| %>
  <%= render partial: 'form', locals: {f: f} %>
<% end %>

<% content_for :footer do %>
  <h2><%= link_to t('forms.buttons.cancel'), @dessert.desserts_category, class: "btn btn-default pull-left" %></h2>
<% end %>